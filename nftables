table ip6 filter {
    set non_arin6 {
        type ipv6_addr
        flags interval
        elements = {
            2001:0200::/23, 2001:0600::/23, 2001:0800::/22, 2001:0c00::/23,
            2001:0e00::/23, 2001:1200::/23, 2001:1400::/22, 2001:1a00::/23,
            2001:1c00::/22, 2001:2000::/19, 2001:4000::/23, 2001:4200::/23,
            2001:4400::/23, 2001:4600::/23, 2001:4a00::/23, 2001:4c00::/23,
            2001:5000::/20, 2001:8000::/19, 2001:a000::/20, 2001:b000::/20,
            2003:0000::/18, 2400:0000::/12, 2800:0000::/12, 2a00:0000::/12,
            2a10:0000::/12, 2c00:0000::/12
        }
    }
}
